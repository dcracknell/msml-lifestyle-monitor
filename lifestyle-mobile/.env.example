# URL where the lifestyle web server/API is running
EXPO_PUBLIC_API_BASE_URL=https://www.msmls.org

# Public origin of the web dashboard (used for linking back to the site)
EXPO_PUBLIC_WEB_APP_ORIGIN=https://www.msmls.org

# Local native app identifiers for device builds (must be unique to your Apple/Google accounts)
EXPO_PUBLIC_IOS_BUNDLE_IDENTIFIER=com.yourname.msml.lifestyle
EXPO_PUBLIC_ANDROID_PACKAGE=com.yourname.msml.lifestyle

# Apple Developer Team ID used for iOS automatic signing (find in Apple Developer account)
APPLE_TEAM_ID=YOUR_TEAM_ID
